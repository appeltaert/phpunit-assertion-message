{
  "name": "appeltaert/phpunit-assertion-message",
  "type": "library",
  "license": "MIT",
  "description": "Enhanced PHPUnit Assertion Messages - Debug assertions without creating a mess.",
  "require": {
    "php": ">=5.6"
  },
  "keywords": [
    "phpunit",
    "testing"
  ],
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Appeltaert\\PAM\\Tests\\": "tests",
      "Appeltaert\\PAM\\": "src"
    },
    "files": ["src/functions.php"]
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "config": {
    "platform": {
      "php": "5.6"
    }
  }
}
